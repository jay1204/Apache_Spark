rdd_1 = sc.parallelize([1,2,3,4,5,6,7])
rdd_2 = sc.textFile('data/mllib/pic_data.txt')
rdd = sc.parallelize([1,2,3,4])
rdd.collect()
rdd.count()
rdd.first()
rdd.take(5)
rdd.take(5)
def add(x,y):
    return x+y

def multiply(x,y):
    return x*y

rdd.reduce(add)
rdd.reduce(multiply)
def square(x):
    return x**2

rdd_squared = rdd.map(square)
rdd_squared.collect()
rdd_squared = rdd.map(lambda x: x**2)
rdd_squared.collect()
def positive(x):
    if x>0:
        return True
    return False

rdd = sc.parallelize([1,2,-3,4,-5,6,7])
rdd_pos = rdd.filter(positive)
rdd_pos.collect()
rdd.filter(lambda x:x>0).collect()
rdd = sc.parallelize([("a",1),("a",2),("b",1)])
rdd_grouped = rdd.groupByKey()
rdd_grouped.collect()
rdd_grouped.map(lambda x:(x[0],list(x[1]))).collect()
rdd = sc.parallelize([2,3,4])
rdd.map(lambda x: range(1,x)).collect()
rdd.flatMap(lambda x: range(1,x)).collect()
